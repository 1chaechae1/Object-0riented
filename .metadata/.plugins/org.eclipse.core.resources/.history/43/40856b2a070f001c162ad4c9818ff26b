package week10;

public class Cylinder {
	double height;
	Circle c1;
	public Cylinder(Circle c1 , double height) {
		super();
		this.height = height;
		this.c1 = c1;
	}
	
	public double getVolume() {
		return c1.getArea()*height;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
