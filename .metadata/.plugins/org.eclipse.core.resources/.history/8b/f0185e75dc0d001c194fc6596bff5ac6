package week03;
public class Test1 {
	public static void main(String[] args) {
		
		System.out.println("byte 최대 = \t " +Byte.MAX_VALUE);
		System.out.println("byte 최소 = \t " +Byte.MIN_VALUE);
		
		System.out.println("");
		System.out.println("short 최대 = \t " +Short.MAX_VALUE);
		System.out.println("short 최소 = \t " +Short.MIN_VALUE);
		
		System.out.println("");
		System.out.println("문자 최대 = \t " +(int)Character.MAX_VALUE);
		System.out.println("문자 최소 = \t " +(int)Character.MIN_VALUE);
		
		System.out.println("");
		System.out.println("정수 최대 = \t " +Integer.MAX_VALUE);
		System.out.println("정수 최소 = \t " +Integer.MIN_VALUE);
		
		System.out.println("");
		System.out.println("long 최대 = \t " +Long.MAX_VALUE);
		System.out.println("long 최소 = \t " +Long.MIN_VALUE);

	}

}
