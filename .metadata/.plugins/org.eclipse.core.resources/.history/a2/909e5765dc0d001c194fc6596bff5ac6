package week03;

public class Test01 {

	public static void main(String[] args) {
		
		System.out.println("float 置企 = \t " +Float.MAX_VALUE);
		System.out.println("float 置社 = \t " +Float.MIN_VALUE);
		
		System.out.println("");
		System.out.println("Double 置企 = \t " +Double.MAX_VALUE);
		System.out.println("Double 置社 = \t " +Double.MIN_VALUE);

	}

}